    "@reduxjs/toolkit": "^1.8.5",
    "concurrently": "7.3.0",
    "json-server": "^0.17.0",
    "next": "12.2.2",
    "next-redux-wrapper": "^7.0.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-redux": "^8.0.2",
    "swiper": "^8.3.2",
    "yarn-check": "^0.0.3"
  },
  "devDependencies": {
    "@testing-library/react": "^12.1.4",
    "@types/node": "^18.7.13",
    "@types/react": "^18.0.17",
    "@vitejs/plugin-react": "1.3.0",
    "autoprefixer": "^10.4.8",
    "cypress": "^10.6.0",
    "eslint": "8.20.0",
    "eslint-config-next": "12.2.2",
    "jsdom": "^19.0.0",
    "postcss": "^8.4.16",
    "start-server-and-test": "^1.14.0",
    "tailwindcss": "^3.1.8",
    "typescript": "^4.8.2",
    "vitest": "^0.22.1"
