{
  "name": "front-end-task",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start:server": "json-server -p 3005 db.json",
    "dev": "concurrently \"yarn start:server\" \"next dev\"",
    "build": "next build",
    "start": "concurrently \"yarn start:server\" \"next start\"",
    "depcheck": "yarn-check -u",
    "lint": "next lint",
    "test": "vitest --reporter verbose",
